{
  "name": "ponder-refactor",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": ".eslintrc.js",
  "scripts": {
    "start": "react-scripts start",
    "start:some": "SET NODE_ENV=development&& concurrently \"npm run devserver\" \"npm run arlocal:some\"",
    "build": "react-scripts build",
    "devserver": "SET NODE_ENV=development&& webpack-dev-server",
    "arlocal:all": "concurrently \"arlocal\" \"npm run seed:all\"",
    "arlocal:some": "concurrently \"arlocal\" \"npm run seed:some\"",
    "seed:all": "node ./seeders/all-episodes",
    "seed:some": "node ./seeders/some-episodes",
    "eject": "react-scripts eject",
    "test": "react-scripts test",
    "test:config": "react-scripts test -- --config jest.config.js",
    "lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
    "jest": "SET NODE_ENV=test&& jest -i",
    "jest:watch": "npm run jest -- --watchAll",
    "cy:run": "SET NODE_ENV=test&& npm run cy -- run",
    "cy:open": "SET NODE_ENV=test&& npm run cy -- open",
    "cy": "cypress",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "GPL-3",
  "dependencies": {
    "@types/react-router-dom": "^5.3.3",
    "arweave": "^1.10.23",
    "bootstrap": "^5.1.2",
    "cy-node-html-label": "^2.0.0",
    "cytoscape": "^3.20.0",
    "cytoscape-dagre": "^2.3.2",
    "cytoscape-panzoom": "^2.5.3",
    "dayjs": "^1.10.7",
    "formik": "^2.2.9",
    "history": "^5.0.1",
    "lodash.clonedeep": "^4.5.0",
    "prop-types": "^15.7.2",
    "react": "^18.0.0",
    "react-bootstrap": "^1.6.4",
    "react-dom": "^18.0.0",
    "react-icons": "^4.3.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "5.0.1",
    "rss-parser": "^3.12.0",
    "sanitize-html": "^1.27.5",
    "styled-components": "^5.3.3",
    "typescript": "^4.6.3",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.11.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/react": "^18.0.3",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.9.1",
    "arlocal": "^1.1.29",
    "babel-jest": "^27.5.1",
    "concurrently": "^6.3.0",
    "cypress": "^9.0.0",
    "cypress-plugin-snapshots": "^1.4.4",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "12.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "jest-date-mock": "^1.0.8",
    "jest-environment-jsdom": "^27.5.1",
    "regenerator-runtime": "^0.13.9"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
