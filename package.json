{
  "name": "podsphere-alpha",
  "private": true,
  "version": "0.0.6",
  "description": "Podsphere is a persistent data store, a graph-visualized discovery service and a discussion platform for Podcasts, built on Arweave.",
  "main": ".eslintrc.js",
  "scripts": {
    "start": "REACT_APP_USE_ARLOCAL=false npm run devserver",
    "startlocal": "concurrently \"REACT_APP_USE_ARLOCAL=true npm run devserver\" \"npm run arlocal:all\"",
    "startlocal:some": "concurrently \"REACT_APP_USE_ARLOCAL=true npm run devserver\" \"npm run arlocal:some\"",
    "build": "react-app-rewired build",
    "devserver": "react-app-rewired start",
    "arlocal:all": "concurrently \"arlocal\" \"npm run seed:all\"",
    "arlocal:some": "concurrently \"arlocal\" \"npm run seed:some\"",
    "seed:all": "REACT_APP_USE_ARLOCAL=true ts-node --script-mode ./seeders/index 'all'",
    "seed:some": "REACT_APP_USE_ARLOCAL=true ts-node --script-mode ./seeders/index 'some'",
    "eject": "react-app-rewired eject",
    "test": "npm run jest && npm run cy:run",
    "jest": "jest -i",
    "jestcov": "jest --coverage -i",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --cache --cache-location node_modules/.cache/.eslintcache",
    "cy:run": "npm run cy -- run",
    "cy:open": "npm run cy -- open",
    "cy": "cypress",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "github.com/Davdxv",
  "license": "GPL-3",
  "engines": {
    "node": "^16.10.0 || >=18.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.3",
    "@mui/material": "^5.10.3",
    "arweave": "^1.11.6",
    "assert": "^2.0.0",
    "axios": "^0.27.2",
    "bootstrap": "^5.2.0",
    "browserify-zlib": "^0.2.0",
    "constants-browserify": "^1.0.0",
    "crypto-browserify": "^3.12.0",
    "cy-node-html-label": "^2.0.0",
    "cytoscape": "^3.22.1",
    "cytoscape-dagre": "^2.4.0",
    "cytoscape-panzoom": "^2.5.3",
    "dayjs": "^1.11.5",
    "dedent": "^0.7.0",
    "fake-indexeddb": "^4.0.0",
    "fflate": "^0.7.3",
    "formik": "^2.2.9",
    "he": "^1.2.0",
    "html-react-parser": "^3.0.4",
    "idb": "^7.0.2",
    "indexeddb-export-import": "^2.1.5",
    "isomorphic-dompurify": "^0.22.0",
    "itunes-store-api": "^2.1.0",
    "path-browserify": "^1.0.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.2",
    "react-error-boundary": "^3.1.4",
    "react-highlight-words": "^0.18.0",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "rss-parser": "^3.12.0",
    "sass": "^1.54.8",
    "stream-browserify": "^3.0.0",
    "styled-components": "^5.3.5",
    "typescript": "^4.8.2",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.4",
    "zod": "^3.17.10"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
