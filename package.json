    "regenerator-runtime": "^0.13.9",
    "ts-node": "^10.9.1"
  },
  "overrides": {
    "@svgr/webpack": "$@svgr/webpack"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
